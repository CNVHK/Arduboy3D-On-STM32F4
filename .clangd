CompileFlags:
  Add:
    - '-ferror-limit=0'
    - '-Wno-implicit-int'
    - '-I${env:TOOLCHAIN_PATH}/arm-none-eabi/include'
    - '-I${env:TOOLCHAIN_PATH}/lib/gcc/arm-none-eabi/13.3.1/include'
    - '-I${env:TOOLCHAIN_PATH}/lib/gcc/arm-none-eabi/13.3.1/include-fixed'
  CompilationDatabase: build/Debug
Diagnostics:
  Suppress:
    - unused-includes
    - unknown_typename
    - unknown_typename_suggest
    - typename_requires_specqual
